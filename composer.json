                "autoload": {
                    "files": [
                        "markdown.php"
                    ]
                }
            }
        }
    ],
    "suggest": {
        "corneltek\/kendo": "~1.0"
    },
    "autoload": {
        "psr-0": {
            "Phifty": "src"
        }
    },
    "version": "2.8.7"
