{
    "name": "corneltek\/phifty-core",
    "require": {
        "php": ">=5.3.0",
        "corneltek\/universal": "~1.3",
        "corneltek\/locale": "~1",
        "corneltek\/universal-cache": "~1.0",
        "corneltek\/fileutil": "~1",
        "corneltek\/roller": "*",
        "corneltek\/actionkit": "*",
        "corneltek\/assettoolkit": "*",
        "corneltek\/lazyrecord": "*",
        "corneltek\/genphp": "~1.2",
        "corneltek\/sessionkit": "~1.0",
        "corneltek\/twig-markdown": "~0.1",
        "corneltek\/sitemap": "~1",
        "swiftmailer\/swiftmailer": "@stable",
        "twig\/twig": "~1.12",
        "twig\/extensions": "1.0.*@dev",
        "symfony\/finder": "~2.2",
        "symfony\/process": "~2.2",
        "symfony\/yaml": "~2.2",
        "symfony\/css-selector": "~2.2",
        "michelf\/php-markdown": "~1",
        "corneltek\/kendo": "~1",
        "phpexcel\/phpexcel": "*",
        "facebook\/php-sdk": "*"
    },
    "require-dev": {
        "firephp\/firephp-core": "dev-master",
        "corneltek\/phpunit-testmore": "dev-master",
        "phpexcel\/phpexcel": "~1.7"
    },
    "repositories": [
        {
            "type": "package",
